<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CustomUploadCareIndigoPink" parent="UploadcareStyle">
        <item name="uploadcareColorAccent">@color/colorAccent</item>
        <item name="uploadcareColorPrimary">@color/colorPrimary</item>
        <item name="uploadcareColorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="CustomUploadCareGreenRed" parent="UploadcareStyle">
        <item name="uploadcareColorAccent">@color/colorRedAccent</item>
        <item name="uploadcareColorPrimary">@color/colorGreenPrimary</item>
        <item name="uploadcareColorPrimaryDark">@color/colorGreenPrimaryDark</item>
    </style>
</resources>
